             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:HTMLPanel>
        <g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE">
            <g:Label ui:field="usernameLabel" styleName="gwt-Label-header"/>
